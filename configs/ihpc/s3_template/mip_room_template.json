{
    "source_path": "data/mip-nerf360/room",
    "images": "images_2",
    "coarse_mesh_path": "output/mipnerf360_s1/room/coarse_mesh.obj",
    "s2_model_path": "output/tmp_mipnerf360_s2/room",
    "s3_model_path": "output/next_tmp_mipnerf360_s3/room_$(expname)",
    "eval": true,
    "s2_iterations": 10000,
    "s3_iterations": 7000,
    "warm_up_iter": 0,

    "aabb_fg_bg": [[[-4.3, -4.5, -11.8], [4.7, 8.5, 7.0]], [[-4.3, -4.5, -11.8], [4.7, 8.5, 7.0]]],
    "gs_per_face": 3,
    "res_fg": 250,
    "res_bg": 0,
    "c2f_steps": [2000, 3000, 4000, 5000],
    "c2f_rate": 1.5,

    "sdf_lr": 0.005,
    "deform_lr": 0.02,
    "sdf_lr_bg": null,
    "deform_lr_bg": null,
    "final_lr_rate": 0.1,
    "texture_lr": 0.01,
    "scale_factor_lr": 0.0005,
    
    "reg_decay_iter": 10000,
    "lambda_sdf": [0.3, 0.1],
    "lambda_flex": [0.1, 0.05],

    "s3_gs_per_face": 6,
    "simplify_nface": 1700000,
    "init_opacity": $(init_opacity),
    "vert_lr": $(vert_lr),
    "feature_lr": $(feature_lr),
    "vert_lr_final": null,
    "opacity_lr": $(opacity_lr),
    "scaling_lr": $(scaling_lr),
    "rotation_lr": $(rotation_lr),
    "lambda_laplace": $(lambda_laplace),
    "lambda_normal_consistency": $(lambda_normal_consistency),
    "use_frustum": true
}