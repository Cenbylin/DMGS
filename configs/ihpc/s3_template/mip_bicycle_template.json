{
    "source_path": "data/mip-nerf360/bicycle",
    "images": "images_4",
    "coarse_mesh_path": "output/mipnerf360_s1/bicycle/coarse_mesh.obj",
    "s2_model_path": "output/tmp_mipnerf360_s2/bicycle",
    "s3_model_path": "output/next_tmp_mipnerf360_s3/bicycle_$(expname)",
    "eval": true,
    "s2_iterations": 10000,
    "s3_iterations": 1500,
    "warm_up_iter": 1000,

    "aabb_fg_bg": [[[-1, -0.3, -1.5], [2, 2.5, 2.5]], [[-19, -15, -19], [19, 6, 19]]],
    "gs_per_face": 3,
    "res_fg": 130,
    "res_bg": 300,
    "c2f_steps": [2000, 3500, 4500, 5500, 6500, 7500],
    "c2f_rate": 1.5,

    "sdf_lr": 0.005,
    "deform_lr": 0.02,
    "sdf_lr_bg": 0.005,
    "deform_lr_bg": 0.02,
    "final_lr_rate": 0.1,
    "texture_lr": 0.003,
    "scale_factor_lr": 0.0005,

    "reg_decay_iter": 10000,
    "lambda_sdf": [0.3, 0.1],
    "lambda_flex": [0.3, 0.05],

    "s3_gs_per_face": 6,
    "simplify_nface": 1700000,
    "init_opacity": 0.1,
    "vert_lr": $(vert_lr),
    "feature_lr": $(feature_lr),
    "opacity_lr": $(opacity_lr),
    "scaling_lr": $(scaling_lr),
    "rotation_lr": $(rotation_lr),
    "lambda_laplace": $(lambda_laplace),
    "lambda_normal_consistency": $(lambda_normal_consistency),
    "use_frustum": true
}