{
    "source_path": "data/mip-nerf360/stump",
    "images": "images_4",
    "coarse_mesh_path": "output/mipnerf360_s1/stump/coarse_mesh.obj",
    "s2_model_path": "output/next_mipnerf360_s2/stump_$(expname)",
    "s3_model_path": "output/tmp_mipnerf360_s3/stump_$(expname)",
    "eval": true,
    "s2_iterations": 6500,
    "s3_iterations": 5000,
    "warm_up_iter": 1000,

    "aabb_fg_bg": $(aabb_fg_bg),
    "gs_per_face": 3,
    "res_fg": $(res_fg),
    "res_bg": $(res_bg),
    "c2f_steps": $(c2f_steps),
    "c2f_rate": 1.5,

    "sdf_lr": $(sdf_lr),
    "deform_lr": $(deform_lr),
    "sdf_lr_bg": $(sdf_lr_bg),
    "deform_lr_bg": $(deform_lr_bg),
    "final_lr_rate": $(final_lr_rate),
    "texture_lr": $(texture_lr),
    "scale_factor_lr": $(scale_factor_lr),

    "reg_decay_iter": 10000,
    "lambda_sdf": $(lambda_sdf),
    "lambda_flex": $(lambda_flex)
